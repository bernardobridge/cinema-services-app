{
  "name": "venues-service",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node start.js",
    "test": "NODE_ENV=development jest --runInBand --forceExit --env=node",
    "pact": "NODE_ENV=development jest --runInBand --forceExit --env=node --testRegex \"/*(.pact.js)\"",
    "pact:publish": "node ./__tests__/pact/publish.js",
    "db-migrate": "knex migrate:latest --env development",
    "db-seed": "knex seed:run --env development",
    "db-drop": "knex migrate:down --env development",
    "db-prepare": "npm run db-migrate && npm run db-seed",
    "db-docker-start": "docker build -t docker-venues-db . && docker run -d -p 5434:5432 docker-venues-db"
  },
  "author": "Bernardo Guerreiro @bernardobridge",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "knex": "^0.20.8",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.6.1",
    "jest": "^25.1.0",
    "nock": "^11.7.2",
    "supertest": "^4.0.2"
  }
}
